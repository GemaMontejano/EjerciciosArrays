//2. Pediremos a través de un bucle 6 números al usuario que almacenaremos en un array
//Imprimiremos por pantalla la longitud del array, el número más alto y el número más bajo
numero = [1, 2, 3, 4, 5, 6]
let mayor = 0
let menor = 0
for (i = 0; i < numero.length; i++) {
    if (numero[i] > mayor) {
        mayor = numero[i]
    }


}
let max = Math.max(...numero)
let min = Math.min(...numero)
console.log(numero.length)
console.log(min)
console.log(max)
